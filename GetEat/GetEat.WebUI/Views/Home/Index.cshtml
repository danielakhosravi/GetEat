                                    <label for="city">Find</label>
                                    <input id="city" class="dropdown-toggle" placeholder="place, city">
                                    <span>|</span>
                                    <label for="cuisine">Kitchen</label>
                                    <input id="cuisine" class="dropdown-toggle" type="text" placeholder="type sushi, french...">
                                @*</div>*@
                                <button class="dropdown-toggle buttonGet" type="submit" value="submit">Find GetEat</button>
                            @*</div>*@
                        </div>
                       </section>
                    <aside class="col-md-2 col-md-pull-8"></aside>
                    <aside class="col-md-2"></aside>
                </div>
            </div>
        </div>
    </div>
</div>

@section scripts {
    <script type="text/javascript" src="~/Scripts/geteat/home.js">

    </script>
}
