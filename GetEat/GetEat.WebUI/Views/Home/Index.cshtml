@{
    ViewBag.Title = "Home Page";
}


<div class="container-fluid">
    <div class="row">

        <div class="screen-bgr">
            <div class="row">
                <div class="transperant">
                  
                    <section class="col-sm-12 col-md-8 col-md-push-2 text-center center-content">
